<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
        <pattern>%date [%level] %logger{15} - %message%n%xException{10}</pattern>
    </encoder>
  </appender>
  <logger name="net.usebox.gemini.server" level="DEBUG" />
  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
